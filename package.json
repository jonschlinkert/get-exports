  "version": "0.1.0",
  "homepage": "https://github.com/jonschlinkert/get-exports",
  "author": {
    "name": "Jon Schlinkert",
    "url": "https://github.com/jonschlinkert"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jonschlinkert/get-exports.git"
  },
  "bugs": {
    "url": "https://github.com/jonschlinkert/get-exports/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/jonschlinkert/get-exports/blob/master/LICENSE-MIT"
    }
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha -R spec"
  },
  "preferGlobal": true,
  "bin": {
    "exp": "./cli.js"
  },
  "devDependencies": {
    "verb-tag-jscomments": ">= 0.2.0",
